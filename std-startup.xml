<aiml version="1.0.1" encoding="UTF-8">
    <!--std-startup.xml-->

    <!--<category>作为AIML的原子级单元-->
    <category>

        <!--匹配用户输入的模式-->
        <!--如果用户输入"LOADAIMLB"-->
        <pattern>LOAD AIML B</pattern>

        <!--<Template>用来响应模式-->
        <!--<learn>是一个aiml文件-->
        <template>
            <learn>basic_chat.aiml</learn>
        </template>

    </category>

</aiml>